<div class="line number1 index0 alt2"><code class="plain">grouped_data </code><code class="keyword">=</code> <code class="plain">dataframe.groupby(</code><code class="string">'listed_in(type)'</code><code class="plain">)[</code><code class="string">'votes'</code><code class="plain">].</code><code class="functions">sum</code><code class="plain">()</code></div>
<div class="line number2 index1 alt1"><code class="plain">result </code><code class="keyword">=</code> <code class="plain">pd.DataFrame({</code><code class="string">'votes'</code><code class="plain">: grouped_data})</code></div>
<div class="line number3 index2 alt2"><code class="plain">plt.plot(result, c</code><code class="keyword">=</code><code class="string">"green"</code><code class="plain">, marker</code><code class="keyword">=</code><code class="string">"o"</code><code class="plain">)</code></div>
